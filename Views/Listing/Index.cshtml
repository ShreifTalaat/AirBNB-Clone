                    </select>
                </div>
                <div id="myMap" class="col-md-7 m-auto" style="position:relative;aspect-ratio: 2/1;"></div>
                <div class=" container row">
                    <div class="col d-none">
                        <label for="longitude">Longitude:</label>
                        <input asp-for="coordinate.X" class="form-control" type="text" id="lon" name="coordinate.X" value="" readonly hidden />
                    </div>
                    <div class="col d-none">
                        <label for="latitude">Latitude:</label>
                        <input asp-for="coordinate.Y" class="form-control" type="text" id="lat" name="coordinate.Y" value="" readonly hidden />
                    </div>
                    <div class="col ">
                        <label for="latitude">Address:</label>
